<div>
    <div>
    <%if(users!=''){%>
    <table border="1px">
    <thead>
    <tr>
    <th>Name</th>
    <th>E-Mail</th>
    <th>Phone</th>
    <th>Action</th>
    </tr>
    </thead>
    <tbody>
    <%users.forEach((row)=>{%>
    <tr>
    <td><%=row.name%></td>
    <td><%=row.email%></td>
    <td><%=row.phone%></td>
    <td><a href="/edit/<%=row._id%>">edit</a><br><a
    href="/delete/<%=row._id%>">delete</a></td>
    </tr>
    <%})%>
    </tbody>
    </table>
    <%}else{%>
    <h1>No users found</h1>
    <%}%>
    </div>
    </div>
    